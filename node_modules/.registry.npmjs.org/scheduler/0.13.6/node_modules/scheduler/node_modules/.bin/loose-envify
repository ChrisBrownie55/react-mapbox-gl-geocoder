#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/Users/christopherbrown/Desktop/Workspace/react-mapbox-gl-geocoder/node_modules/.registry.npmjs.org/loose-envify/1.4.0/node_modules/loose-envify/cli.js/node_modules:/Users/christopherbrown/Desktop/Workspace/react-mapbox-gl-geocoder/node_modules/.registry.npmjs.org/loose-envify/1.4.0/node_modules/loose-envify/node_modules:/Users/christopherbrown/Desktop/Workspace/react-mapbox-gl-geocoder/node_modules/.registry.npmjs.org/loose-envify/1.4.0/node_modules:/Users/christopherbrown/Desktop/Workspace/react-mapbox-gl-geocoder/node_modules/.registry.npmjs.org/loose-envify/node_modules:/Users/christopherbrown/Desktop/Workspace/react-mapbox-gl-geocoder/node_modules/.registry.npmjs.org/node_modules:/Users/christopherbrown/Desktop/Workspace/react-mapbox-gl-geocoder/node_modules:/Users/christopherbrown/Desktop/Workspace/node_modules:/Users/christopherbrown/Desktop/node_modules:/Users/christopherbrown/node_modules:/Users/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../loose-envify/1.4.0/node_modules/loose-envify/cli.js" "$@"
  ret=$?
else 
  NODE_PATH="/Users/christopherbrown/Desktop/Workspace/react-mapbox-gl-geocoder/node_modules/.registry.npmjs.org/loose-envify/1.4.0/node_modules/loose-envify/cli.js/node_modules:/Users/christopherbrown/Desktop/Workspace/react-mapbox-gl-geocoder/node_modules/.registry.npmjs.org/loose-envify/1.4.0/node_modules/loose-envify/node_modules:/Users/christopherbrown/Desktop/Workspace/react-mapbox-gl-geocoder/node_modules/.registry.npmjs.org/loose-envify/1.4.0/node_modules:/Users/christopherbrown/Desktop/Workspace/react-mapbox-gl-geocoder/node_modules/.registry.npmjs.org/loose-envify/node_modules:/Users/christopherbrown/Desktop/Workspace/react-mapbox-gl-geocoder/node_modules/.registry.npmjs.org/node_modules:/Users/christopherbrown/Desktop/Workspace/react-mapbox-gl-geocoder/node_modules:/Users/christopherbrown/Desktop/Workspace/node_modules:/Users/christopherbrown/Desktop/node_modules:/Users/christopherbrown/node_modules:/Users/node_modules:/node_modules" node  "$basedir/../../../../../../loose-envify/1.4.0/node_modules/loose-envify/cli.js" "$@"
  ret=$?
